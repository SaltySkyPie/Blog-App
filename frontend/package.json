{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-gql": "concurrently \"npm run dev\" \"npm run generate-gql-watch\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "format": "prettier --config .prettierrc --write \"src/**/*.ts?(x)\"",
    "generate-gql": "graphql-codegen --config ./src/graphql/codegen.config.ts",
    "generate-gql-watch": "graphql-codegen --config ./src/graphql/codegen.config.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/x-data-grid": "^6.18.7",
    "@uiw/react-md-editor": "^4.0.2",
    "axios": "^1.6.3",
    "formik": "^2.4.5",
    "graphql": "^16.8.1",
    "i18next": "^23.7.12",
    "react": "^18.2.0",
    "react-auth-kit": "^2.12.7",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.1",
    "react-share": "^5.0.3",
    "react-toastify": "^9.1.3",
    "rehype-sanitize": "^6.0.0",
    "timeago-react": "^3.0.6",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-checker": "^0.6.0"
  },
  "engines": {
    "node": "20"
  }
}
